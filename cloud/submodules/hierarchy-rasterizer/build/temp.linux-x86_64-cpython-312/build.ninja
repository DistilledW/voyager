ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda/bin/nvcc

cflags = -pthread -B /opt/miniconda/envs/3dgs/compiler_compat -fno-strict-overflow -Wsign-compare -DNDEBUG -O2 -Wall -fPIC -O2 -isystem /opt/miniconda/envs/3dgs/include -fPIC -O2 -isystem /opt/miniconda/envs/3dgs/include -fPIC -I/opt/miniconda/envs/3dgs/lib/python3.12/site-packages/torch/include -I/opt/miniconda/envs/3dgs/lib/python3.12/site-packages/torch/include/torch/csrc/api/include -I/opt/miniconda/envs/3dgs/lib/python3.12/site-packages/torch/include/TH -I/opt/miniconda/envs/3dgs/lib/python3.12/site-packages/torch/include/THC -I/usr/local/cuda/include -I/opt/miniconda/envs/3dgs/include/python3.12 -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=_C -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++17
cuda_cflags = -I/opt/miniconda/envs/3dgs/lib/python3.12/site-packages/torch/include -I/opt/miniconda/envs/3dgs/lib/python3.12/site-packages/torch/include/torch/csrc/api/include -I/opt/miniconda/envs/3dgs/lib/python3.12/site-packages/torch/include/TH -I/opt/miniconda/envs/3dgs/lib/python3.12/site-packages/torch/include/THC -I/usr/local/cuda/include -I/opt/miniconda/envs/3dgs/include/python3.12 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -Xcompiler -fno-gnu-unique -I/workspace/code/cloud/submodules/hierarchy-rasterizer/third_party/glm/ -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=_C -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++17
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc --generate-dependencies-with-compile --dependency-output $out.d $cuda_cflags -c $in -o $out $cuda_post_cflags





build /workspace/code/cloud/submodules/hierarchy-rasterizer/build/temp.linux-x86_64-cpython-312/cuda_rasterizer/backward.o: cuda_compile /workspace/code/cloud/submodules/hierarchy-rasterizer/cuda_rasterizer/backward.cu
build /workspace/code/cloud/submodules/hierarchy-rasterizer/build/temp.linux-x86_64-cpython-312/cuda_rasterizer/forward.o: cuda_compile /workspace/code/cloud/submodules/hierarchy-rasterizer/cuda_rasterizer/forward.cu
build /workspace/code/cloud/submodules/hierarchy-rasterizer/build/temp.linux-x86_64-cpython-312/cuda_rasterizer/rasterizer_impl.o: cuda_compile /workspace/code/cloud/submodules/hierarchy-rasterizer/cuda_rasterizer/rasterizer_impl.cu
build /workspace/code/cloud/submodules/hierarchy-rasterizer/build/temp.linux-x86_64-cpython-312/ext.o: compile /workspace/code/cloud/submodules/hierarchy-rasterizer/ext.cpp
build /workspace/code/cloud/submodules/hierarchy-rasterizer/build/temp.linux-x86_64-cpython-312/rasterize_points.o: cuda_compile /workspace/code/cloud/submodules/hierarchy-rasterizer/rasterize_points.cu






